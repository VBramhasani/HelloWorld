pipeline{
	aget {
		label 'WinAgent'
	tools { 
        maven 'MAVEN_HOME' 
        jdk 'JAVA_Home'
		Git 'default'
		}
	}
	
	stages{
		stage(SCM Checkout){
			stpes{
			sh "rm -rf -f HelloWorld"
			sh "git clone https://github.com/VBramhasani/HelloWorld.git"
			}
		}
		stage(Maven build){
			steps{
			sh "mvn clean install package"
			}
		}
		stage('deploy') {
			steps {
			sh "mvn package -f HelloWorld"
				}
			}
		
	}
}
